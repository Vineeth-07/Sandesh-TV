    top: 0;
    left: 0;
    height: 100%;
    width: 200px; /* Set your preferred width */
    background-color: #ffffff; /* Add your preferred background color */
    box-shadow: 20px 20px 60px #00000041, inset -20px -20px 60px #ffffff40;
    z-index: 2;
  }
  .sidebar.open {
    display: block;
  }
  .content-container {
    transition: margin-left 0.3s ease;
    margin-left: 0;
    z-index: 2; /* Adjust the margin to match the sidebar width */
  }
  /* body {
    margin: 0;
    padding: 0;
  } */
  #closeSidebar:hover {
    transform: scale(1.2); /* Zoom in by 10% on hover */
  }
  .topButton {
    background-color: #aee9e5;
    transition: transform 0.3s ease-in-out;
  }
  .topButton:hover {
    transform: scale(1.1);
    background-color: #0d9488;
  }
  #closeSidebar {
    transition: transform 0.3s; /* Add a transition effect for the transform property */
  }
  /* .group:hover .hidden {
    display: block;
  } */
  @media (max-width: 640px) {
    .sidebar {
      display: none; /* Hide the sidebar on small screens by default */
      position: fixed;
      top: 0;
      left: 0;
      height: 100%;
      width: 60%; /* Make the sidebar full width on small screens */
      background-color: #ffffff;
      box-shadow: 20px 20px 60px #00000041, inset -20px -20px 60px #ffffff40;
      z-index: 2;
    }

    .sidebar.open {
      display: block;
    }
    .content-container {
      margin-left: 0;
      flex: auto;
    }
  }
